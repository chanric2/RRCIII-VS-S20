Module MathExample

    Sub Main()

        'Add integer 2 with integer 2
        'Addition Operation
        Console.WriteLine("2 + 2 =")
        Console.WriteLine(2 + 2)
        Console.ReadLine()

        'Subtract integer 7 with integer 2
        'Subraction Operation
        Console.WriteLine("7 - 2 =")
        Console.WriteLine(7 - 2)
        Console.ReadLine()

        'Multiply integer 3 with integer 2
        'Multiplication Operation
        Console.WriteLine("3 * 2 =")
        Console.WriteLine(3 * 2)
        Console.ReadLine()

        'Divide integer 18 with integer 2
        'Division w/ remainder Operation
        Console.WriteLine("18 / 2 =")
        Console.WriteLine(18 / 2)
        Console.ReadLine()

        'Divide integer integer 3 with integer 2
        'Division w/o remainder Operation
        Console.WriteLine("3 \ 2 =")
        Console.WriteLine(3 \ 2)
        Console.ReadLine()

        'Mod integer integer 25 with integer 2
        'Modulus only remainder Operation
        Console.WriteLine("25 Mod 3 =")
        Console.WriteLine(25 Mod 3)
        Console.ReadLine()

        'Exponet integer integer 2 with integer 8
        'Exponential Operation
        Console.WriteLine("2 ^ 8 =")
        Console.WriteLine(2 ^ 8)
        Console.ReadLine()

    End Sub

End Module
